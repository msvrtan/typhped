parameters:
	featureToggles:
		deadCatchesRule: true
		missingReturnRule: true
		noopRule: true
		subtractableTypes: true
		unreachableStatement: true
		yieldRules: true
		validateParameters: true
		inferPrivatePropertyTypeFromConstructor: true
	bootstrap: tests/bootstrap.php
	ignoreErrors:
	    - '#Cannot access property $name on SimpleXMLElement|null.#'
	    - '#Instanceof between Typhped\\Structure\\Token\\TokenVariable and Typhped\\Tokenizer\\Manual\\TokenString will always evaluate to false.#'
	excludes_analyse: []
includes:
	- ../vendor/phpstan/phpstan-phpunit/extension.neon
	- ../vendor/phpstan/phpstan-phpunit/rules.neon
	- ../vendor/phpstan/phpstan-webmozart-assert/extension.neon
	- ../vendor/phpstan/phpstan-deprecation-rules/rules.neon
	- ../vendor/phpstan/phpstan-strict-rules/rules.neon
	- ../vendor/phpstan/phpstan-mockery/extension.neon



